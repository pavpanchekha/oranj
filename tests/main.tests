## Test input and output fascilities

# output() function
>>> output(5 ^ 15)
30517578125

# io variable
>>> io << 5 ^ 15
30517578125

# if
>>> a = 0
>>> if a == 0 {
>>> io << "YAY"
>>> } else {
>>> io << "NAY"
>>> }
YAY

# while loop
>>> a = 0
>>> while a < 10 {a = a + 1}
>>> io << a
10

# break
>>> a = 0
>>> while 1 {break}
>>> io << "DONE"
DONE

# while forever
>>> while {break}
>>> io << "DONE"
DONE
