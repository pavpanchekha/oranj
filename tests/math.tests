
#!../oranj -r

## Test literal syntax

# Integers
>>> io << 1
1
>>> io << 100000000000000000000000
100000000000000000000000

# Other bases
>>> io << 0b11
3
>>> io << 0h11
17
>>> io << 0a11
37
>>> io << 0o11
9
>>> io << 0d11
11
>>> io << 0s11
18

# Decimals
## Note that decimals should be rendered with three digits of precision in
## oranj
>>> io << 1.0
1.000
>>> io << 0.0
0.000
>>> io << 1.
1.000
>>> io << .1
0.100

# Arithmetic
>>> io << 42 + 17
59
>>> io << 42 - 17
25
>>> io << 42 * 17
714
>>> io << 42 / 17
2.470
>>> io << 42 ^ 17
3937657486715347520027492352

# Order of operations
>>> io << (1 + 2) / (3 + 4) + 5
5.428
>>> io << (4 + 10 / 2) / 9
1
>>> io << 3 - (5 - (7 + 1)) ^ 2 * (-5) + 3
51

# Logic
>>> io << (5 == 17)
False
>>> io << (5 != 17)
True
>>> io << (5 < 17)
True
>>> io << (5 > 17)
False
>>> io << (5 <= 17)
True
>>> io << (5 >= 17)
False
>>> io << (5 => 17)
False
>>> io << (5 =< 17)
True
